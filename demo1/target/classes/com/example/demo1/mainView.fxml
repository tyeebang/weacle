<AnchorPane id="mainViewBg" prefHeight="640.0" prefWidth="360.0" stylesheets="@mainView.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.MainViewController">
   <children>
      <Pane layoutX="30.0" layoutY="20.0" prefHeight="140.0" prefWidth="300.0">
         <children>
            <Text fill="#ff719e" layoutX="40.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text=" weacle" wrappingWidth="220.0">
